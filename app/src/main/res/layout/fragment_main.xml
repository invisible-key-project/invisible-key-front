<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:name = "com.example.transparentkey_aos.MainFragment">

    <Button
        android:id="@+id/btn_embed"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="140dp"
        android:layout_marginBottom="290dp"
        android:background="@drawable/style_main_btn"
        android:fontFamily="@font/pretendard_bold"
        android:text="워터마크\n삽입"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btn_extract"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_extract"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:text="워터마크\n인증"
        android:textSize="16dp"
        android:fontFamily="@font/pretendard_bold"
        android:background="@drawable/style_main_btn"
        app:layout_constraintBottom_toBottomOf="@+id/btn_embed"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btn_embed"
        app:layout_constraintTop_toTopOf="@+id/btn_embed" />

    <Button
        android:id="@+id/btn_manage"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="64dp"
        android:text="워터마크\n관리"
        android:textSize="16dp"
        android:fontFamily="@font/pretendard_bold"
        android:background="@drawable/style_main_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_embed" />
</androidx.constraintlayout.widget.ConstraintLayout>